{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-console": "off",
    "padded-blocks": "off",
    "no-plusplus": "off"
  }
}
